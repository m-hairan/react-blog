{
  "name": "blog",
  "description": "Mo's Blog",
  "version": "0.3.0",
  "author": "Mu Haibao<mhbseal@163.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/mhbseal/blog"
  },
  "scripts": {
    "start.noauth": "node --harmony app.js noauth",
    "start": "node --harmony app.js",
    "start.dev": "node-dev --harmony app.js",
    "dev": "npm run mongod.dev & npm run start.dev",
    "pm2.start": "NODE_ENV=production pm2 start app.js --name blog --node-args='--harmony' --watch",
    "pm2.reload": "NODE_ENV=production pm2 reload app.js --name blog --node-args='--harmony' --watch",
    "nginx.start": "/usr/local/nginx/sbin/nginx",
    "nginx.reload": "/usr/local/nginx/sbin/nginx -s reload",
    "chmod": "chmod -R +x bin",
    "mongod.dev": "bin/mongod.dev.sh",
    "mongodump.dev": "bin/mongodump.dev.sh",
    "mongorestore.dev": "bin/mongorestore.dev.sh",
    "mongod.prod": "bin/mongod.prod.sh",
    "mongodump.prod": "bin/mongodump.prod.sh",
    "mongorestore.prod": "bin/mongorestore.prod.sh",
    "tar.dev": "bin/tar.dev.sh",
    "tar.prod": "bin/tar.prod.sh",
    "scp.dev": "bin/scp.dev.sh",
    "data.dev": "npm run scp.dev && npm run tar.dev",
    "data.prod": "npm run mongodump.prod && npm run tar.prod"
  },
  "dependencies": {
    "body-parser": "^1.13.3",
    "co": "^4.6.0",
    "compression": "^1.5.2",
    "cookie-session": "^1.2.0",
    "ejs": "^2.3.1",
    "express": "^4.13.3",
    "mongoose": "^4.0.2",
    "morgan": "^1.6.1",
    "multer": "^1.0.1",
    "requirejs": "^2.1.18",
    "serve-favicon": "^2.3.0"
  }
}
