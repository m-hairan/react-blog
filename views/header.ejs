	<meta name="description" content="<%- blogInfo.description %>" />
	<meta name="keywords" content="<%- blogInfo.keywords %>" />
	<!-- 缩放控制 -->
	<meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0" />
	<!-- 删除默认的苹果工具栏和菜单栏 -->
	<meta name="apple-mobile-web-app-capable" content="yes" />
	<link rel="stylesheet" href="/static/styles/main.css" />
	<!--[if lt IE 9]>
	<script src="/static/scripts/html5.js"></script>
	<script src="/static/scripts/css3-mediaqueries.min.js"></script>
	<![endif]-->
</head>
<body>
	<header class="header">
		<h1><a href="/" class="logo"><%- blogInfo.title %></a></h1>
		<div class="icon-menu"></div>
		<div class="icon-search"></div>
		<nav id="nav">
			<a href="/" id="home">主页</a>
			<% articleTypes.forEach(function(v) { %>
			<a href="/<%- v.path %>"><%- v.name %></a>
			<% }) %>
		</nav>
		<div id="search" class="search">
			<input type="text" placeholder="keywords" class="form-control search_input">
			<a href="javascript:void(0)" class="btn search_btn">GO</a>
		</div>
	</header>