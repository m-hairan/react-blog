<% include header %>
<div class="main" id="articleType">
	<table class="table1">
		<tr>
			<td class="td1">&nbsp;</td>
			<td><h2><%- xData._id ? '编辑' : '新增' %></h2></td>
		</tr>
		<tr>
			<td class="td1">名称：</td>
			<td><input type="text" name="name" class="form-control" value="<%- xData.name %>" /></td>
		</tr>
		<tr>
			<td class="td1">路径：</td>
			<td><input type="text" name="path" class="form-control" value="<%- xData.path %>" /></td>
		</tr>
		<tr>
			<td class="td1">是否启用：</td>
			<td>
				<select name="enabled" class="form-control">
					<option value="1" selected="selected">是</option>
					<option value="0">否</option>
				</select>
			</td>
		</tr>
		<tr>
			<td class="td1">&nbsp;</td>
			<td><a href="javascript:void(0)" class="btn" data-id="<%- xData._id %>">确定</a></td>
		</tr>
	</table>
</div>
<% include footer %>