<% include header %>
<div class="main" id="articleTypeList">
	<a href="<%- adminPath %>articleType" class="btn">新增</a>
	<div class="ad_table_box">
		<table class="ad_table">
			<tr>
				<th>序号</th>
				<th>名称</th>
				<th>路径</th>
				<th>是否启用</th>
				<th>操作</th>
			</tr>
			<% xData.forEach(function(type) { %>
			<tr>
				<td><%- type._id %></td>
				<td><%- type.name %></td>
				<td><%- type.path %></td>
				<td><%- type.enabled %></td>
				<td>
					<a href="<%- adminPath %>articleType?id=<%- type._id %>">编辑</a>&nbsp;&nbsp;
					<a href="javascript:void(0)" data-href="<%- adminPath %>articleType?id=<%- type._id %>" class="js_delete">删除</a>
				</td>
			</tr>
			<% }) %>
		</table>
		<% include ../common/pageList %>
	</div>
</div>
<% include footer %>